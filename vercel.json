{
  "version": 2,
  "builds": [
    { "src": "build.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "alias": ["chrisocphoto.vercel.app"]
}
